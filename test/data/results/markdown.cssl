* {font-size: 12pt;font-weight: normal;font-style: none;text-decoration: none;color: black;}
h1::before {content: "# ";color: rgb(85,85,85);}
h2::before {content: "## ";color: rgb(85,85,85);}
h3::before {content: "### ";color: rgb(85,85,85);}
h4::before {content: "#### ";color: rgb(85,85,85);}
h5::before {content: "##### ";color: rgb(85,85,85);}
h6::before {content: "###### ";color: rgb(85,85,85);}
em:before, em:after {content: "_";color: rgb(85,85,85);}
strong:before, strong:after, b:before, b:after {content: "**";color: rgb(85,85,85);}
code:before, code:after, kbd:before, kbd:after, var:before, var:after, samp:before, samp:after {content: "`";color: rgb(85,85,85);}
blockquote:before, blockquote p:before {content: "> ";color: rgb(85,85,85);}
blockquote p:first-child:before {content: "";}
ul li {list-style: none;}
ul li:before {content: "- ";color: rgb(85,85,85);}
pre > code {text-indent: 3em;}
hr:before {content: "  ---  ";color: rgb(85,85,85);}
hr {text-align: center;color: transparent;height: 0;display: block;line-height: 0;margin: 2em 0;}
hgroup * {margin: 0;}
hgroup {margin: 1em 0;}
a:before {content: "[";color: rgb(85,85,85);}
a:after {content: "](" attr(href) ")";color: rgb(85,85,85);}
pre:before, pre:after {content: "\A    ```\A    ";color: rgb(85,85,85);}
body {font-family: monospace;width: 42em;margin: 0 auto;}
